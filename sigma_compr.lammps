units lj 
dimension 3 
atom_style molecular 
boundary p p p 

region box block 7.22872 12.7713 7.22872 12.7713 7.22872 12.7713 
create_box 1 box & 
bond/types 1 & 
angle/types 1 & 
extra/bond/per/atom 6 &
extra/angle/per/atom 10 &

read_data sigma.data add append
velocity all create 0.0 4928459 rot yes dist gaussian   

comm_modify mode single cutoff 1.7550000000000001 vel yes 

pair_style lj/cut 0.6732
pair_modify shift yes 
pair_coeff 1 1 1.0 0.6 0.6732

bond_style fene
special_bonds fene
bond_coeff 1 30.0 1.5 1.0 0.65

angle_style cosine
angle_coeff 1 20.0

neigh_modify every 1 delay 5 check yes

thermo 10
thermo_modify norm no 
dump mydmp all custom 1 dump_no_bar.lammpstrj id type x y z

min_style cg
minimize 1.0e-20 1.0e-20 1000000 1000000
print "first min"
fix deform_box all deform 1 x scale 0.8 units box 
timestep 0.01
run 100
unfix deform_box

fix deform_box all deform 1 y scale 0.8 units box 
timestep 0.01
run 100
unfix deform_box

fix deform_box all deform 1 z scale 0.8 units box 
timestep 0.01
run 100
unfix deform_box

min_style cg
minimize 1.0e-20 1.0e-20 1000000 1000000
print "second min"
fix deform_box all deform 1 x scale 1.07 units box 
timestep 0.01
run 100
unfix deform_box

fix 1 all box/relax y -3.616705 z -3.616705 vmax 0.1
minimize 1.0e-20 1.0e-20 1000000 1000000